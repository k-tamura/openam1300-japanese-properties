<!DOCTYPE ModuleProperties PUBLIC "=//iPlanet//Authentication Module Properties XML Interface 1.0 DTD//EN"
        "jar://com/sun/identity/authentication/Auth_Module_Properties.dtd">


<ModuleProperties moduleName="DeviceIdSave" version="1.0" >
    <Callbacks length="0" order="1" timeout="600" header="#WILL NOT BE SHOWN#" />

    <Callbacks length="1" order="2" timeout="600" header="信頼済みのデバイスに登録" >
        <ChoiceCallback>
            <Prompt>信頼済みのデバイスに登録しますか?</Prompt>
            <ChoiceValues>
                <ChoiceValue>
                    <Value>Yes</Value>
                </ChoiceValue>
                <ChoiceValue isDefault="true">
                    <Value>No</Value>
                </ChoiceValue>
            </ChoiceValues>
        </ChoiceCallback>
    </Callbacks>

    <Callbacks length="1" order="3" timeout="600" header="信頼済みのデバイス名" >
        <NameCallback>
            <Prompt>信頼済みのデバイス名?</Prompt>
        </NameCallback>
    </Callbacks>
</ModuleProperties>
